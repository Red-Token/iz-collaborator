{
	"semi": false,
	"printWidth": 120,
	"bracketSameLine": false,
	"svelteSortOrder": "options-styles-scripts-markup",
	"arrowParens": "avoid",
	"bracketSpacing": false,
	"trailingComma": "none",
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"overrides": [{"files": "*.svelte", "options": {"parser": "svelte"}}],
	"endOfLine": "lf",
	"proseWrap": "preserve"
}
